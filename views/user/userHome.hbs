     function myFunction() {
        var input, filter, cards, cardContainer, h5, title, i;
        input = document.getElementById("myFilter");
        filter = input.value.toUpperCase();
        cardContainer = document.getElementById("myItems");
        cards = cardContainer.getElementsByClassName("product-item");
        myman = document.getElementsByClassName("myman")
        console.log("input"+input)
        console.log("filter"+filter)
        console.log("cardContainer"+cardContainer)
        console.log("cards"+cards)

        for (i = 0; i < cards.length; i++) {
            title = cards[i].querySelector(".manufacture-product h5.nameOfProduct");
            console.log("title"+title)
            if (title.innerText.toUpperCase().indexOf(filter) > -1) {
                myman[i].style.display = "";
            } else {
                myman[i].style.display = "none";
            }
        }
    }
</script>



