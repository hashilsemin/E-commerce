<div class="my-account-wrapper pb-20">
    <div class="container-fluid">
        <div class="section-title left-aligned">
            <h3>Your address</h3>
        </div>
        <div class="row">

            {{#each address}}
            <div class="col-lg-3 col-md-4 col-sm-6">
                <div class="product-item mb-30">
                    <div class="product-caption" style="font-size: large;">
                        <div class="manufacture-product">
                            <p style="font-size: x-large;">{{this.address}}</p>
                        </div>
                        <div class="product-name">
                            <h4 style="font-size: large;">{{this.city}}</h4>
                        </div>
                        <div class="product-name">
                            <h4 style="font-size: large;">{{this.province}}</h4>
                        </div>
                        <div class="product-name">
                            <h4 style="font-size: large;">{{this.zip}}</h4>
                        </div>
                        <div class="product-name">
                            <h4 style="font-size: large;">{{this.mobile}}</h4>
                        </div>


<a class="btn btn-warning" href="/editAddress/{{this._id}}">Edit</a>    
<a class="btn btn-danger" href="/deleteAddress/{{this._id}}"  onclick="return confirm('Are you sure you want to delete this address? ')">Delete</a>

                    </div>
                </div> <!-- end single grid item -->
            </div>
            {{/each}}
        </div>
    </div>
</div>
