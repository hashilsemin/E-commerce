<!-- Static Table Start -->
<div class="data-table-area mg-b-15">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="sparkline13-list shadow-reset">
          <div class="sparkline13-hd">
            <div class="main-sparkline13-hd">
              <h1>Order <span class="table-project-n">Data</span> Table</h1>
              <div class="sparkline13-outline-icon">
              
                
              </div>
            </div>
          </div>
          <div class="sparkline13-graph">
            <div class="datatable-dashv1-list custom-datatable-overright">
              <div id="toolbar">
                <select class="form-control">
                  <option value="">Export Basic</option>
                  <option value="all">Export All</option>
                  <option value="selected">Export Selected</option>
                </select>
              </div>
              <table id="table" data-toggle="table" data-pagination="true" data-search="true" data-show-columns="true"
                data-show-pagination-switch="true" data-show-refresh="true" data-key-events="true"
                data-show-toggle="true" data-resizable="true" data-cookie="true" data-cookie-id-table="saveId"
                data-show-export="true" data-click-to-select="true" data-toolbar="#toolbar">
                <thead>
                  <tr>
                    <th data-field="state" data-checkbox="true"></th>
                    <th data-field="date" data-editable="true">Date</th>

                    <th data-field="orderid" data-editable="true">User name</th>
                    <th data-field="phone" data-editable="true">City</th>
                    <th data-field="name" data-editable="true">Product</th>

                    <th data-field="company" data-editable="true">Price</th>
                    <th data-field="payemnt" data-editable="true">Payment</th>
                    <th data-field="status" data-editable="true">Shipping status</th>

                    {{!-- <th data-field="company" data-editable="true">Status</th> --}}

                    <th data-field="action">Action</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each order}}
                  <tr>
                    <td></td>
                    <td id="{{this._id}}{{this.product.product.product}}">{{this.date}}</td>
                    <script>
                      func('{{this._id}}{{this.product.product.product}}')

                      function func(id) {
                        d = document.getElementById(id).innerHTML

                        var index = d.lastIndexOf(':') + 3
                        console.log(d.substring(0, index))
                        document.getElementById(id).innerHTML = d.substring(0, index)
                      }

                    </script>

                    <td>{{this.deliveryDetails.fname}}</td>
                    <td>{{this.deliveryDetails.city}}</td>
                    <td>{{this.product.product.product}}</td>

                    <td>{{this.product.totalPrice}}</td>
                    <td>{{this.payment}}</td>

                    <td>
                      {{#if this.product.product.cancelled}}
                      ORDER CANCELLED
                      {{else}}
                      {{#if this.product.product.ship}}
                      {{this.product.product.ship}}
                      {{else}}
                      Pending for shipping
                      {{/if}}
                      {{/if}}
                    </td>
                    {{!-- <td>{{this.block}}</td> --}}
                    <td class="datatable-ct">

                      <a href="/vendor/viewOrder/{{this._id}}/{{this.product._id}}" class="btn btn-primary">View
                        More</a>



                      {{#if this.product.product.cancelled}}
                     
                        
                      
                     
                      {{else}}
                         <a href="/vendor/cancelOrder/{{this._id}}/{{this.product.product._id}}"
                        class="btn btn-warning">Cancel Order</a>
                         {{#if this.product.product.ship}}
                      <a href="/vendor/cancelShip/{{this._id}}/{{this.product.product._id}}" class="btn btn-danger"
                        onclick="return confirm('Cancel {{name}} ? ')">Cancel Shipping</a>
                      {{else}}
                      <a href="/vendor/ship/{{this._id}}/{{this.product.product._id}}" class="btn btn-success"
                        onclick="return confirm('Confirm shipping {{name}} ? ')">Ship</a>
                      {{/if}}
                      {{/if}}
                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Static Table End -->
